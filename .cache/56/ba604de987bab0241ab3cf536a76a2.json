pp.js","original":{"line":54,"column":22},"name":"content"},{"generated":{"line":39,"column":29},"source":"app.js","original":{"line":54,"column":0}},{"generated":{"line":39,"column":32},"source":"app.js","original":{"line":56,"column":0}}],"sources":{"app.js":"const container = document.getElementById(\"root\");\nconst ajax = new XMLHttpRequest();\nconst content = document.createElement(\"div\");\nconst NEWS_URL = \"https://api.hnpwa.com/v0/news/1.json\";\n// 바뀔 가능성이 있는 변수는 뺴주는게 좋다 URL\nconst CONTENT_URL = \"https://api.hnpwa.com/v0/item/@id.json\";\n\najax.open(\"GET\", NEWS_URL, false);\n\n// 동기적으로 처리하겠다.\najax.send();\n// 데이터를 가져오겠다\n\n//응답 값을 객체로 바꿔보자 (preview 탭)\n// JSON 데이터를 객체로 바꾸는 도구는 'json.parse)이다\n\nconst newsFeed = JSON.parse(ajax.response);\nconst ul = document.createElement(\"ul\");\n\n// 객체화되어 있을 것 (axios에서는 이 기능을 자동으로 처리해줌)\n\nwindow.addEventListener(\"hashchange\", () => {\n  const id = location.hash.substring(1);\n\n  // 쓰고 싶은 위치값부터 그 이후부터만 나오게\n  // 1은 \"#\"을 제외한 나머지의 문자열만 나오게 되는 것이다.\n  ajax.open(\"GET\", CONTENT_URL.replace(\"@id\", id), false);\n  ajax.send();\n\n  const newsContent = JSON.parse(ajax.response);\n  const title = document.createElement(\"h1\");\n\n  title.innerHTML = newsContent.title;\n\n  content.appendChild(title);\n\n  console.log(newsContent);\n\n  // 문자열 함수 중에 값을 대체해주는 replace 함수가 있다\n});\n\nfor (let i = 0; i < 10; i++) {\n  const li = document.createElement(\"li\");\n  const a = document.createElement(\"a\");\n\n  a.href = `#${newsFeed[i].id}`;\n  a.innerHTML = `${newsFeed[i].title} (${newsFeed[i].comments_count})`;\n\n  li.appendChild(a);\n  ul.appendChild(li);\n}\n\ncontainer.appendChild(ul);\ncontainer.appendChild(content);\n\n// 똑같은 코드 반복은 좋지 않다.\n"},"lineCount":null}},"error":null,"hash":"5177368d3106ac8e84159cac12118a5a","cacheData":{"env":{}}}